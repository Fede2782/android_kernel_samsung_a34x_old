		rear_sensor_id = <120>;     /* SENSOR_NAME_S5KHM6 */
		front_sensor_id = <121>;    /* SENSOR_NAME_IMX616 */
		rear2_sensor_id = <210>;    /* SENSOR_NAME_IMX355 */
