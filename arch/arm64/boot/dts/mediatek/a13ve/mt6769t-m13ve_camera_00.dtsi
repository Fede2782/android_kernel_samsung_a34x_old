};
&s2mpb03_0_l5 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2900000>;
};
&s2mpb03_0_l6 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2900000>;
};
&s2mpb03_0_l7 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2900000>;
};

/*************************
 *     Cmaera I2C
 *************************/

&i2c1 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <400000>;
	mediatek,use-open-drain;

	camera_main_eeprom_mtk:camera_main_eeprom@50 {
		compatible = "mediatek,camera_main_eeprom";
		reg = <0x50>;
		status = "okay";
	};

	camera_main_af_mtk:camera_main_af@18 {
		compatible = "mediatek,camera_main_af";
		reg = <0x18>;
		status = "okay";
	};

};

&i2c4 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <1000000>;
	mediatek,use-open-drain;
	camera_main_mtk:camera_main@34{
		compatible = "mediatek,camera_main";
		reg = <0x34>;
		status = "okay";
	};
};

&i2c8 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <400000>;
	mediatek,use-open-drain;
	camera_sub_mtk:camera_sub@5a {
		compatible = "mediatek,camera_sub";
		reg = <0x5a>;
		status = "okay";
	};
	camera_main_two_mtk:camera_main_two@7e{
		compatible = "mediatek,camera_main_two";
		reg = <0x7e>;
		status = "okay";
	};

	camera_main_three_mtk:camera_main_three@6e {
		compatible = "mediatek,camera_main_three";
		reg = <0x6e>;
		status = "okay";
	};
};

/*************************
 *     Flash LED
 *************************/
 
&i2c3 {
	sm5714@49 {
		sm5714-fled {
			status = "okay";
			compatible = "siliconmitus,sm5714-fled";
			flash-en-gpio = <&pio 159 0>;
			torch-en-gpio = <&pio 110 0>;

			flash-brightness = <0xA>; //0x0(300mA) ~ 0xF(1500mA) : 50mA Step
			preflash-brightness = <0x4>;
			torch-brightness = <0x4>; //0x0(50mA) ~ 0x7(225mA) : 25mA Step
			factory_current = <0x6>; //0x0(300mA)
			flashlight_current = <0 1 2 5 7>;
			timeout = <0xff>;
		};
	};
};
